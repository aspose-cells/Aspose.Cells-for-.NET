    <link rel="stylesheet" href="~/xspread/jquery-ui.min.css" />

    <script type="text/javascript" src="~/xspread/jquery-2.1.1.js"></script>

    <script type="text/javascript" src="~/xspread/jquery-ui.min.js"></script>
    <script>
        $(function () {
            $("input").checkboxradio();
        });
        function openfile(name) {
            const url = ($('#radio-1:checked').val() ? '/GridJs2/Uidtml' : '/GridJs2/Xspreadtml') + '?filename=' + name;
            window.open(url, "_blank");
        }
    </script>
    <style>
        body {
            overflow-y:hidden;
        }
    </style>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container" style="text-align: center;">
